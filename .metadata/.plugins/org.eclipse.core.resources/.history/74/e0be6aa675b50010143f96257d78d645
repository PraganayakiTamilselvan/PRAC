package outer;

import java.util.Scanner;

import PACK1.Cal;

public class bc {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s="Welcome";
		s=s.toLowerCase();
		for(int i=0;i<s.length();i++) {
			int count=1;
			for(int j=1;j<s.length();j++) {
				if (s.charAt(i)==s.charAt(j)){
					count++;
				}
			}
			System.out.println(s.charAt(i)+ count);
		}
		
	}
}
