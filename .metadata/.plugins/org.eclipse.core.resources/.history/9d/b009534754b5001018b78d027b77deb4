package com.example;

public class Demo {

	public static void main(String[] args) {
		int a=27;
		if(a<10) {
			System.out.println(a);
		}
		else {
		int n=a/16;
		int rem = a % 16;
		String s="";
		s=s+n;
		if(rem==10) {
			s=s+'A';
		}
		else if(rem==11) {
			s=s+'B';
		}
		else if(rem==12) {
			s=s+'C';
		}
		else if(rem==13) {
			s=s+'D';
		}
		else if(rem==14) {
			s=s+'E';
		}
		else if(rem==15) {
			s=s+'F';
		}
		else {
			s=s+rem;
		}			
		System.out.println(s);	
		}
	}
}
