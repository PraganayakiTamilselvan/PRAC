package PACK1;
public class a {
			int x=20000;
			void bal(){
				System.out.println("Account balance:"+ this.x );
			}
			void withdraw(int x) {
				if(x<this.x) {
				int w = x-this.x;
				System.out.println("Withdrawn amount:"+x);
				System.out.println("Balance:"+w);
				}
				else {
					System.out.println("Insufficient Amount");
				}
			}
			void deposit(int x) {
				int d=x + this.x;
				System.out.println("Amount Deposited:"+x);
				System.out.println("Balance:"+d);
			}
			public static void main(String[] args) {
				a b =new a();
				b.bal();
				b.withdraw(100000);
				b.deposit(5000);
				
				
			}

}