package com.example;

import java.time.LocalDate;

public class Date {
public static void main(String[] args) {
	LocalDate ld = LocalDate.now();
	System.out.println(ld);
	System.out.println(ld.isAfter(ld));
	System.out.println(ld.minusDays(-6));
