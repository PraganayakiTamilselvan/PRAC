package com.example;
import java.util.*;
public class Arr {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<5;i++) {
			for(int j=0;j<5;j++) {
				if(i==1||j==1||i==4||j==4) {
					System.out.print("*");
				}
				else {
					System.out.print("   ");
				}
			}
			System.out.println();
		}
	}		
	}

