package com.example;
import java.util.*;
public class Arr {
	public static void main(String[] args) {
		int n=9;
		int d;
		int rem=0;
		while(n!=0) {
			d=n%8;
			rem=(rem*10)+n;
			n=n/8;
		}
		System.out.println(rem);
	}
}
