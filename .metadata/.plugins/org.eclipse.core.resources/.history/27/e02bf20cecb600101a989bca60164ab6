package com.example;
import java.util.*;
public class Arr {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=5;
		int arr[]= {30,20,10,50,40};
		for(int i=0;i<n;i++) {
			for(int j=0;j<n-i-1;j++) {
				if(arr[j]>arr[j+1]) {
					int temp=arr[j];
					arr[j]=arr[j+1];
					arr[j+1]=temp;

				}
			}
			
			}
		for(int i=0;i<n-1;i++) {
			System.out.println(arr[i]);
		}
		
	}
}
