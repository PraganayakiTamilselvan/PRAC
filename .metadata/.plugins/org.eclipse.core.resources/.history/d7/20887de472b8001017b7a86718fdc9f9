package com.example;
import java.util.*;
class A{
	A(){
		System.out.println("hello im A");
	}
	A(int v){
		System.out.println("Im first");
	}
}
public class Arr extends A{
	Arr(){
		super(4);
		System.out.println("Im 2nd");
	}
	Arr (int v){
		;
		System.out.println(v);
	}
	public static void main(String[] args) {
		new Arr();
	}
		
	}
