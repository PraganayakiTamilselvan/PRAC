package com.example;
import java.util.*;
public class Arr {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
//		int arr[][]= new int[n][m];
//		for(int i=0;i<n ;i++) {
//			for(int j=0;j<m;j++) {
//			arr[i][j]=sc.nextInt();
//		}
//		}
		for(int i=0;i<=n-1;i++) {
			for(int j=0;j<=n-1;j++) {
				if(i==0&&j==1&&i==1) {
				System.out.print("*");
				}
				else {
					System.out.print("  ");
				}
			}
			System.out.println();
		}
	}
}
