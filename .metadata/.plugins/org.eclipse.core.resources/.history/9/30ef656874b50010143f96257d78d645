			int a =0;
			s=s.toLowerCase();
			for(int i=0;i<s.length();i++) {
				char c = s.charAt(i);
				int count=0;
				for(int j=1;j<s.length();j++) {
					if (c[i] == c[j]) {
						count++;
					}
				}
				a+=count;
				System.out.println('c'+ count);
			}
			
